{
  "name": "frontendweekly",
  "version": "0.0.1",
  "description": "Frontend Weeklyでは国内・海外のフロントエンド関連情報を厳選し毎週お届けします。",
  "scripts": {
    "build": "cross-env ELEVENTY_ENV=production eleventy",
    "build:clean": "rimraf dist/*",
    "build:function": "netlify functions:build --src functions",
    "build:postcss": "postcss src/_postcss/main.pcss -o src/_includes/assets/styles/main.css --env production",
    "build:production": "run-s build:clean build:function build:postcss build",
    "dev": "cross-env ELEVENTY_ENV=development eleventy --serve",
    "dev:netlify": "cross-env ELEVENTY_ENV=development netlify dev",
    "start": "npm run dev",
    "post:new": "node _task/node-trello-contents.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand --watch"
  },
  "author": "Yuya Saito <hello@studiomohawk.com> (https://github.com/studiomohawk)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/frontendweekly/frontendweekly.git"
  },
  "keywords": [
    "eleventy",
    "netlify"
  ],
  "bugs": {
    "url": "https://github.com/frontendweekly/frontendweekly/issues"
  },
  "homepage": "https://github.com/frontendweekly/frontendweekly#readme",
  "dependencies": {
    "@11ty/eleventy": "^0.11.0",
    "@11ty/eleventy-cache-assets": "^2.0.2",
    "@11ty/eleventy-plugin-rss": "^1.0.7",
    "@11ty/eleventy-plugin-syntaxhighlight": "^3.0.1",
    "@frontendweekly/collection-postfeed": "^0.0.6",
    "@frontendweekly/collection-posts": "^0.0.6",
    "@frontendweekly/molle": "0.0.18",
    "autoprefixer": "^9.8.0",
    "cssnano": "^4.1.10",
    "dotenv": "^8.2.0",
    "node-jq": "^1.11.1",
    "postcss": "^7.0.31",
    "postcss-cli": "^7.1.1",
    "postcss-import": "^12.0.1",
    "postcss-load-config": "^2.1.0",
    "postcss-preset-env": "^6.7.0",
    "sanitize-html": "^1.25.0",
    "twitter": "^1.7.1"
  },
  "devDependencies": {
    "@testing-library/dom": "^7.7.0",
    "@testing-library/jest-dom": "^5.9.0",
    "cross-env": "^7.0.2",
    "eslint": "^7.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-import-resolver-jest": "^3.0.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jest": "^23.13.2",
    "eslint-plugin-jest-dom": "^2.1.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-testing-library": "^3.1.4",
    "fast-glob": "^3.2.2",
    "husky": "^4.2.5",
    "jest": "^26.0.1",
    "jest-date-mock": "^1.0.8",
    "jest-fetch-mock": "^3.0.3",
    "jest-runner-eslint": "^0.9.0",
    "jest-watch-select-projects": "^2.0.0",
    "lint-staged": "^10.2.7",
    "netlify-cli": "^2.52.0",
    "npm-run-all": "^4.1.5",
    "posthtml": "^0.13.0",
    "posthtml-urls": "^1.0.0",
    "prettier": "^2.0.5",
    "pretty-quick": "^2.0.1",
    "qoa": "^0.2.0",
    "rimraf": "^3.0.2",
    "signale": "^1.4.0",
    "stylelint": "^13.5.0",
    "stylelint-config-prettier": "^8.0.1",
    "stylelint-config-recess-order": "^2.0.4",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-prettier": "^1.1.2"
  }
}
