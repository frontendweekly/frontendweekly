{
  "name": "frontendweekly",
  "version": "0.0.1",
  "description": "Frontend Weeklyでは国内・海外のフロントエンド関連情報を厳選し毎週お届けします。",
  "scripts": {
    "build": "cross-env ELEVENTY_ENV=production eleventy",
    "build:clean": "rimraf dist/*",
    "build:function": "netlify functions:build --src functions",
    "build:postcss": "postcss src/_postcss/main.pcss -o src/_includes/assets/styles/main.css --env production",
    "build:production": "run-s build:clean build:function build:postcss build",
    "dev": "cross-env ELEVENTY_ENV=development eleventy --serve",
    "dev:netlify": "cross-env ELEVENTY_ENV=development netlify dev",
    "start": "npm run dev",
    "post:new": "node _task/node-trello-contents.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand --watch"
  },
  "author": "Yuya Saito <hello@studiomohawk.com> (https://github.com/studiomohawk)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/frontendweekly/frontendweekly.git"
  },
  "keywords": [
    "eleventy",
    "netlify"
  ],
  "bugs": {
    "url": "https://github.com/frontendweekly/frontendweekly/issues"
  },
  "engines": {
    "node": ">=14.15.0"
  },
  "homepage": "https://github.com/frontendweekly/frontendweekly#readme",
  "dependencies": {
    "@11ty/eleventy": "^0.11.1",
    "@11ty/eleventy-cache-assets": "^2.0.4",
    "@11ty/eleventy-plugin-rss": "^1.0.9",
    "@11ty/eleventy-plugin-syntaxhighlight": "^3.0.4",
    "@frontendweekly/collection-postfeed": "^0.0.9",
    "@frontendweekly/collection-posts": "^0.0.9",
    "@frontendweekly/molle": "0.0.29",
    "autoprefixer": "^10.1.0",
    "cssnano": "^4.1.10",
    "dotenv": "^8.2.0",
    "node-jq": "^1.11.3",
    "postcss": "^8.2.2",
    "postcss-cli": "^8.3.1",
    "postcss-import": "^14.0.0",
    "postcss-load-config": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "sanitize-html": "^2.3.0",
    "twitter": "^1.7.1",
    "posthtml": "^0.15.1",
    "posthtml-urls": "^1.0.0"
  },
  "devDependencies": {
    "@frontendweekly/eslint-config-molle": "^0.0.2",
    "@frontendweekly/jest-preset-molle": "^0.0.12",
    "@frontendweekly/stylelint-config-molle": "^0.0.3",
    "@frontendweekly/browserslist-config-molle": "^0.0.1",
    "cross-env": "^7.0.3",
    "eslint": "^7.17.0",
    "fast-glob": "^3.2.4",
    "husky": "^4.3.6",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.3",
    "netlify-cli": "^2.70.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0",
    "qoa": "^0.2.0",
    "rimraf": "^3.0.2",
    "signale": "^1.4.0",
    "stylelint": "^13.8.0"
  }
}
